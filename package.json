{
  "name": "@masxxiii/nestjs",
  "version": "1.0.0",
  "description": "NestJS starter kit",
  "contributors": [
    "Masood Zafar <masxafar@gmail.com>"
  ],
  "keywords": [
    "rest",
    "backend",
    "service",
    "starter-kit"
  ],
  "license": "MIT",
  "homepage": "https://github.com/masxxiii/NestJS#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/masxxiii/NestJS.git"
  },
  "bugs": {
    "url": "https://github.com/masxxiii/NestJS/issues"
  },
  "prisma": {
    "schema": "src/frameworks/services/database/schema.prisma"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config jest.json",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node build/main",
    "clean": "rm -rf node_modules",
    "build": "nest build",
    "prebuild": "rm -rf build",
    "prepare": "husky install",
    "generate:doc": "npx compodoc -p tsconfig.doc.json --theme postmark -n \"Backend Service\"",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@nestjs/cli": "^9.5.0",
    "@nestjs/common": "^9.4.2",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.4.2",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "^1.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.4.2",
    "@nestjs/schedule": "^2.2.2",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "^4.15.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "lodash": "^4.17.21",
    "passport": "^0.6.0",
    "passport-headerapikey": "^1.2.2",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@compodoc/compodoc": "^1.1.19",
    "@darraghor/eslint-plugin-nestjs-typed": "^3.21.2",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.4.2",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.2.5",
    "@types/passport-http": "^0.3.9",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.19.0",
    "eslint-plugin-unicorn": "^47.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prisma": "^4.15.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
